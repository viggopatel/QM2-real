        "import pandas as pd\n",
        "import re\n",
        "\n",
        "# Assume df is your DataFrame after cleaning latitude/longitude\n",
        "\n",
        "# Group by location and sum attacks\n",
        "df_grouped = dfsmall.groupby(['Latitude', 'Longitude'])['Pirate attacks on boats'].sum().reset_index()\n",
        "# 'Value' column is assumed to contain the number of attacks"
      ],
      "metadata": {
        "id": "jXJa4AzP34d6"
