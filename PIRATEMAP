    {
      "cell_type": "code",
      "source": [
        "from ipyleaflet import Map, CircleMarker\n",
        "\n",
        "# Calculate center using grouped data\n",
        "center = (df_grouped['Latitude'].mean(), df_grouped['Longitude'].mean())\n",
        "\n",
        "m = Map(center=center, zoom=2)\n",
        "\n",
        "for index, row in df_grouped.iterrows():\n",
        "    marker = CircleMarker(\n",
        "        location=(row['Latitude'], row['Longitude']),\n",
        "        radius=int(row['Pirate attacks on boats'] / 20),  # Convert to int\n",
        "        color='blue',\n",
        "        fill_color='blue',\n",
        "        fill_opacity=0.6\n",
        "    )\n",
        "    m.add_layer(marker)\n",
        "\n",
        "display(m)\n",
        "m.save('my_map.html', title='My Map')"
